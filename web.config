  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.SqlConnectionFactory, EntityFramework" />
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
  </entityFramework>
  <appSettings>
    <!-- other setting here -->
    <add key="owin:AppStartup" value="Brief.Data.IdentityConfig" />
  </appSettings>

  <connectionStrings>
    <clear />
    <add name="BriefContextConnection"
     providerName="System.Data.SqlClient.SqlClientFactory"
     connectionString="Server=(localdb)\\mssqllocaldb;Database=Brief;Trusted_Connection=True;MultipleActiveResultSets=true" />
  </connectionStrings>
</configuration>

